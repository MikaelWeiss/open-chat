{
  "$schema": "../gen/schemas/mobile-schema.json",
  "identifier": "mobile",
  "description": "Capability for mobile platforms (iOS/Android)",
  "platforms": ["iOS", "android"],
  "windows": ["main"],
  "permissions": [
    "core:default",
    "core:event:allow-emit",
    "core:event:allow-listen",
    "opener:default",
    "shell:default",
    "shell:allow-open",
    "store:default",
    "store:allow-get",
    "store:allow-set",
    "store:allow-clear",
    "store:allow-save",
    "store:allow-load",
    "sql:default",
    "sql:allow-load",
    "sql:allow-select",
    "sql:allow-execute",
    "keychain:default",
    "keychain:allow-get-password",
    "keychain:allow-set-password",
    "keychain:allow-delete-password",
    "dialog:default",
    "dialog:allow-open",
    "dialog:allow-ask",
    "dialog:allow-message",
    "process:default",
    "process:allow-restart",
    "fs:default",
    "fs:allow-read-file"
  ]
}