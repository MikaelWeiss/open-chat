{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "core:window:allow-start-dragging",
    "opener:default",
    "store:default",
    "store:allow-get",
    "store:allow-set",
    "store:allow-clear",
    "store:allow-save",
    "store:allow-load",
    "keyring:default",
    "keyring:allow-get-password",
    "keyring:allow-set-password",
    "keyring:allow-delete-password"
  ]
}
